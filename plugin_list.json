{
  "plugins": [
    {
      "name": "UI Customizer",
      "description": "Allows customization of the OctoPrint interface, adding flexibility to adapt the layout and visuals to fit your workflow.",
      "github_url": "https://github.com/LazeMSS/OctoPrint-UICustomizer",
      "install_command": "~/oprint/bin/pip install https://github.com/LazeMSS/OctoPrint-UICustomizer/archive/master.zip",
      "documentation": "https://plugins.octoprint.org/plugins/uicustomizer/",
      "category": "UI Enhancement",
      "dependencies": [],
      "compatibility": "OctoPrint 1.4+",
      "tags": ["UI", "Customization"]
    },
    {
      "name": "Navbar Temp",
      "description": "Displays hotend, bed, and SoC temperatures directly in the navbar, helping you keep track of thermal parameters easily.",
      "github_url": "https://github.com/imrahil/OctoPrint-NavbarTemp",
      "install_command": "~/oprint/bin/pip install https://github.com/imrahil/OctoPrint-NavbarTemp/archive/master.zip",
      "documentation": "https://plugins.octoprint.org/plugins/navbartemp/",
      "category": "Temperature Monitoring",
      "dependencies": [],
      "compatibility": "OctoPrint 1.3+",
      "tags": ["Temperature", "Monitoring"]
    },
    {
      "name": "Slicer Thumbnails",
      "description": "Displays image thumbnails of G-code files directly within OctoPrint's file manager, making it easier to manage and select prints.",
      "github_url": "https://github.com/jneilliii/OctoPrint-SlicerThumbnails",
      "install_command": "~/oprint/bin/pip install https://github.com/jneilliii/OctoPrint-SlicerThumbnails/archive/master.zip",
      "documentation": "https://plugins.octoprint.org/plugins/slicerthumbnails/",
      "category": "File Management",
      "dependencies": [],
      "compatibility": "OctoPrint 1.4+",
      "tags": ["File Management", "Thumbnails"]
    },
    {
      "name": "Firmware Updater",
      "description": "Updates your printer firmware directly from OctoPrint, eliminating the need for specialized flashing tools.",
      "github_url": "https://github.com/OctoPrint/OctoPrint-FirmwareUpdater",
      "install_command": "~/oprint/bin/pip install https://github.com/OctoPrint/OctoPrint-FirmwareUpdater/archive/master.zip",
      "documentation": "https://plugins.octoprint.org/plugins/firmwareupdater/",
      "category": "Firmware Management",
      "dependencies": [],
      "compatibility": "OctoPrint 1.3+",
      "tags": ["Firmware", "Update"]
    },
    {
      "name": "Exclude Region",
      "description": "Allows selective cancellation of regions during printing, helping save on filament if only a part of the print fails.",
      "github_url": "https://github.com/bradcfisher/OctoPrint-ExcludeRegion",
      "install_command": "~/oprint/bin/pip install https://github.com/bradcfisher/OctoPrint-ExcludeRegion/archive/master.zip",
